<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pluslab">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_pluslab"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_pluslab"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CitasDetallesActivity"></activity>
        <activity android:name=".CitasEditarActivity" />
        <activity android:name=".CitasRegistrarActivity" />
        <activity android:name=".CitasActivity" />
        <activity android:name=".ServiciosDetallesActivity" />
        <activity android:name=".ServiciosPacienteActivity" />
        <activity android:name=".ServiciosEditarActivity" />
        <activity android:name=".ServiciosRegistrarActivity" />
        <activity android:name=".ServiciosAdminActivity" />
        <activity android:name=".MisdatosEditarPacienteActivity" />
        <activity android:name=".MisdatosEditarAdminActivity" />
        <activity android:name=".MisdatosPacienteActivity" />
        <activity android:name=".MisdatosAdminActivity" />
        <activity android:name=".MenuPrincipal" />
        <activity android:name=".RegistrarPacienteActivity" />
        <activity android:name=".InicioSesionActivity" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name=".MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>